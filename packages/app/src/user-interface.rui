        "stateModel": {
          "type": "type",
          "value": "string"
        },
        "value": {
          "type": "string",
          "value": "Hello"
        }
      }
    }
  ],
  "children": [
    {
      "id": "canvas",
      "component": "Panel",
      "props": {
        "header": {
          "type": "string",
          "value": "Header"
        }
      },
      "children": [
        {
          "component": "p",
          "children": [
            "State Value: {user.value}"
          ]
        },
        {
          "id": "testButton",
          "component": "Button",
          "props": {
            "label": {
              "type": "string",
              "value": "Foo"
            }
          },
          "events": {
            "onClick": {
              "type": "functionReference",
              "value": "testButtonClick"
            }
          }
        }
      ]
    }
  ]
